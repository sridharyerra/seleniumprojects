package JavaMethods;

public class methodreturnvalue {

	public static void main(String[] args, Object system) {
		// TODO Auto-generated method stub
    sum(200, 300);
    int res;
    if (res>500)
    	System.out.println("result is grethan 500");
    else 
    	system.out.println("result is less than 500");
	
	
  public static int sum(int x, int y){
	  int res;
	  res=x+y;
	  return res;
	  
  }
}
